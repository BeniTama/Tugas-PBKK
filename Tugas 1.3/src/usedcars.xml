<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="car1" class="co.pbkk.Car">
		<property name="id" value="101"/>
		<property name="brand" value="Toyota"/>
		<property name="condition" value="Used"/>
		<property name="model" value="Prado"/>
	</bean>

	<bean id="car2" class="co.pbkk.Car">
		<property name="id" value="102"/>
		<property name="brand" value="Suzuki"/>
		<property name="condition" value="Used"/>
		<property name="model" value="Alto"/>
	</bean>

</beans>